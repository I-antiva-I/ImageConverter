{
  "PNG":
  {
    "extensions":       [".png"],
    "transformations":  ["JPEG", "BMP", "ICO"]
  },

  "JPEG":
  {
    "extensions":       [".jpg", ".jpeg"],
    "transformations":  ["PNG", "BMP"]
  },

  "BMP":
  {
    "extensions":       [".bmp"],
    "transformations":  ["JPEG", "PNG"]
  },

  "ICO":
  {
    "extensions":       [".ico"],
    "transformations":  []
  }
}
